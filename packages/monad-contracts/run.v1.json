{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Run Record v1",
  "type": "object",
  "required": [
    "id",
    "start",
    "config",
    "artifacts"
  ],
  "properties": {
    "id": {
      "type": "string"
    },
    "start": {
      "type": "string",
      "format": "date-time"
    },
    "end": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time"
    },
    "config": {
      "type": "object"
    },
    "artifacts": {
      "type": "object"
    }
  }
}